<!-- ELECCION SOBRE EL SEXO +++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->

<category>
<pattern>Si</pattern>
<that> es hombre </that>
<template>
	<think>
		<set name="sexo-persona">Hombre</set>
		<set var="contador">1</set>
		<set var="nombre">
			<srai>ITERATE <get name="lista_nombre"/></srai>
		</set>
	
		<set var="iguales">	
			<srai> COMPARE <map name="genero"> <get var="nombre"> </get> </map> WITH <get name="sexo-persona" /> </srai>
		</set>
	</think>
		<condition var="nombre">
				
		<li value="end"></li>
		
		<li>
			<condition var="iguales">

			<li value="YES">
				<think>
				<set var="nombre">
					<srai> NEXT </srai>
				</set>
					
				<set var="iguales">	
					<srai> COMPARE <map name="genero"> <get var="nombre"> </get> </map> WITH <get name="sexo-persona"/> </srai>
				</set>
			</think>
				
			</li>
			<li value="NO">
				<think>
				<set var="contador">
					<srai>FINDITEM <get var="nombre" /> IN <get name="lista_nombre"/> </srai>
				</set>
				<set name="lista_nombre">
					<srai> REMOVEITEM <get var="contador"/> IN <get name="lista_nombre"/>
				</srai>
				</set>

				<set var="nombre">
					<srai>ITERATE <get name="lista_nombre"/></srai>
				</set>

				<set var="iguales">	
					<srai> COMPARE <map name="genero"> <get var="nombre"> </get> </map> WITH <get name="sexo-persona"/></srai>
				</set>
				</think>
			</li>
			</condition>
			<loop/>
		</li>
	</condition>
	<think> 		
		<set var="contador">1</set>
 	</think>
 	<condition var="contador">
		<li><value><srai> COUNT <get name="lista_nombre"/></srai></value>
			 Ya se quien es, es <srai>TOP <get name="lista_nombre"/> </srai> 
		</li>
		<li>
 			Tiene gafas?
		</li>
	</condition>
</template>
</category>

